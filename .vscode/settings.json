{
    "files.associations": {
        "*.reg": "ini",
        "**/lib/newdesktop/*.txt": "markdown",
        "**/features/customPwshProfile/git-aliases.txt": "shellscript"
    },
    "[ini]": {
        "files.encoding": "utf16le",
        "editor.wordWrap": "off"
    },
    "[powershell]": {
        "files.encoding": "utf16le",
        "editor.wordWrap": "off"
    },
    "[md]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
